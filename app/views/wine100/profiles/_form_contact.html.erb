<h3>联系人信息</h3>
		<div class="controls">
		  <%= f.text_field :contact_name, :placeholder => '姓名', :class => 'block box' %>
		  <span class="red">*</span>
		</div>
		<div class="controls">
		  <%= f.text_field :contact_job, :placeholder => '职位', :class => 'block box' %>
		</div>

		<div class="controls">
		  <%= f.text_field :contact_phone, :placeholder => '电话' %>
		</div>

		<div class="controls">
		  <%= f.email_field :contact_email, :placeholder => '邮箱' %>
		</div>